<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AutoTabLayout">
        <attr name="auto_textSize_base_width" format="boolean"></attr>
    </declare-styleable>
    <declare-styleable name="FlowLayout">
        <attr name="hor_space" format="dimension"/>
        <attr name="ver_space" format="dimension"/>
        <attr name="useMinWidth" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="TBButton">
        <attr name="photocircular" format="boolean"/>
        <attr name="photoWidth" format="integer"/>
        <attr name="photoHeight" format="integer"/>
        <attr name="tbTextColor" format="color"/>
        <attr name="tbTextSelColor" format="color"/>
        <attr name="tbTextSize" format="integer"/>
        <attr name="tbText" format="string"/>
        <attr name="select" format="boolean"/>
        <attr name="photoscr" format="reference"/>
        <attr name="photoselscr" format="reference"/>
        <attr name="tbSpace" format="integer"/>
    </declare-styleable>
    <declare-styleable name="TBTextView">
        <attr name="topTextColor" format="color"/>
        <attr name="topTextSize" format="integer"/>
        <attr name="bottomTextColor" format="color"/>
        <attr name="bottomTextSize" format="integer"/>
        <attr name="topBottomSpace" format="integer"/>
        <attr name="topText" format="string"/>
        <attr name="bottomText" format="string"/>
    </declare-styleable>
</resources>